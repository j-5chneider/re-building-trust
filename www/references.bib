}

@techreport{wingenetal.2019,
  title = {No {{Replication}}, No {{Trust}}? {{How Low Replicability Influences Trust}} in {{Psychology}}},
  shorttitle = {No {{Replication}}, No {{Trust}}?},
  author = {Wingen, Tobias and Berkessel, Jana and Englich, Birte},
  year = {2019},
  month = feb,
  institution = {{Open Science Framework}},
  doi = {10.31219/osf.io/4ukq5},
  abstract = {In the current psychological debate, low replicability of psychological findings is a central topic. While the discussion about the replication crisis has a huge impact on psychological research, we know less about how it impacts lay people's trust in psychology. In this paper, we examine whether low replicability damages public trust and whether this damage can be repaired. Study 1 and 2 provide correlational and experimental evidence that low replicability reduces public trust in psychological science. Additionally, Studies 3, 4, and 5 evaluate the effectiveness of commonly used trust-repair strategies, such as information about increased transparency (Study 3), explanations for low replicability (Study 4), or recovered replicability (Study 5). Despite high statistical power, none of these strategies significantly repaired trust. Overall, our studies highlight the crucial importance of replicability for public trust and suggest that commonly used trust-repair strategies might be ineffective in light of low replicability.},
  file = {C\:\\Users\\Juergen Schneider\\Zotero\\storage\\JK2J2YCH\\Wingen et al. - 2019 - No Replication, no Trust How Low Replicability In.pdf},
  keywords = {Open Science,Open Science Practices,trustworthiness},
  type = {Preprint}
}

