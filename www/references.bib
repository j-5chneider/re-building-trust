  journal = {ProQuest LLC},
  keywords = {Autism,Behavior Disorders,Computer Uses in Education,Disadvantaged Schools,Elementary School Students,Emotional Disturbances,Grade 4,Grade 5,Instructional Materials,Learner Engagement,Mild Disabilities,Paper (Material),Persuasive Discourse,Pervasive Developmental Disorders,Rural Schools,Student Attitudes,Students with Disabilities,Writing (Composition)},
