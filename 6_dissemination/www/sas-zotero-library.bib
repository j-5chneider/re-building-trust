@article{kluger1996,
  title = {The Effects of Feedback Interventions on Performance: {{A}} Historical Review, a Meta-Analysis, and a Preliminary Feedback Intervention Theory.},
  author = {Kluger, Avraham N. and DeNisi, Angelo},
  year = {1996},
  volume = {119},
  pages = {254--284},
  issn = {0033-2909},
  doi = {10.1037/0033-2909.119.2.254},
  abstract = {Since the beginning of the century, feedback interventions (FIs) produced negative--but largely ignored--effects on performance. A meta-analysis (607 effect sizes; 23,663 observations) suggests that FIs improved performance on average ( d = .41) but that over one-third of the FIs decreased performance. This finding cannot be explained by sampling error, feedback sign, or existing theories. The authors proposed a preliminary FI theory (FIT) and tested it with moderator analyses. The central assumption of FIT is that FIs change the locus of attention among 3 general and hierarchically organized levels of control: task learning, task motivation, and meta-tasks (including self-related) processes. The results suggest that FI effectiveness decreases as attention moves up the hierarchy closer to the self and away from the task. These findings are further moderated by task characteristics that are still poorly understood. (PsycINFO Database Record (c) 2013 APA, all rights reserved). (journal abstract)},
  archivePrefix = {arXiv},
  eprint = {1011.1669v3},
  eprinttype = {arxiv},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Kluger et al/kluger_et_al_1996_the_effects_of_feedback_interventions_on_performance_-_a_historical_review,_a.pdf},
  isbn = {190393429X},
  journal = {Psychological Bulletin},
  number = {2},
  pmid = {19335571}
}

@article{klusmann2006,
  title = {Berufliche {{Beanspruchung}} Angehender {{Lehrkr{\"a}fte}}: {{Die Effekte}} von {{Pers{\"o}nlichkeit}}, P{\"a}dagogischer {{Vorerfahrung}} Und Professioneller {{Kompetenz}}},
  author = {Klusmann, Uta and Kunter, Mareike and Voss, Thamar and Baumert, Jurgen J{\"u}rgen},
  year = {2006},
  volume = {26},
  pages = {275--290},
  issn = {1010-0652},
  doi = {10.1024/1010-0652/a000078},
  abstract = {The transition to work is an important phase in the process of teachers' professional development. However, there is evidence that not all teachers deal successfully with the demands of the transition resulting in high levels of occupational stress and burnout symptoms. The present study investigates first the development of emotional exhaustion and job satisfaction in the first two years of beginning teachers. Second, we investigate the effects of personality traits, previous educational experiences, pedagogical content knowledge, knowledge about classroom behavior and self-regulatory skills on student teachers emotional exhaustion and job satisfaction. The data consists of more than 500 beginning teachers in their practical phase of teacher education, who were examined in a multi-cohort longitudinal study. The results suggest that emotional exhaustion increases significantly in the first year of teaching practice, while it remains stable in the second year Compared to a sample of experienced teachers, beginning teachers report similar emotional exhaustion but higher job satisfaction. Personality traits as well as aspects of professional knowledge and educational experience explained interindividual differences in the development of exhaustion and satisfaction over one year. The implications of the present results for the professional development of teachers are discussed. (PsycINFO Database Record (c) 2013 APA, all rights reserved) (journal abstract)},
  journal = {Zeitschrift f{\"u}r p{\"a}dagogische Psychologie},
  keywords = {emotional exhaustion job satisfaction beginning te},
  number = {4}
}

