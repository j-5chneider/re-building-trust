@article{harant2015,
  title = {Die {{Problematik}} Der Affirmatio von {{Herkommen}} Und {{Fortschritt}} Als {{Fluchtpunkte}} von {{Bildungsdenken}}.},
  author = {Harant, Martin},
  year = {2015},
  volume = {61},
  pages = {497--516},
  issn = {0044-3247},
  abstract = {Der Beitrag rekonstruiert den bildungstheoretischen Antagonismus einer affirmatio des Herkommens versus einer affirmatio des Fortschritts zwischen Litt und Spranger auf der einen und Blankertz auf der anderen Seite. Dabei wird zun\dbend{}chst die Verh\dbend{}ltnisbestimmung von Herkommen und Fortschritt bei Litt und Spranger rekonstruiert und gezeigt, welche bildungstheoretische Beurteilung sie nach sich zieht. Sodann wird die gleichsam kontr\dbend{}re Beurteilung dieser Verh\dbend{}ltnisbestimmung bei Blankertz in die Sicht genommen. Abschlie\dbend{}end wird gezeigt, inwiefern beide affirmativen Fluchtpunkte zu theoretischen und legitimatorischen Problemstellungen f\dbend{}hren, und als Fazit eine bildungstheoretische Perspektive in Aussicht gestellt, die sich zu beiden Fluchtpunkten gleicherma\dbend{}en abst\dbend{}ndig verh\dbend{}lt. (DIPF/Orig.).;;;The contribution reconstructs the education-theoretical antagonism of an affirmation of provenance versus an affirmatio of progress between Litt and Spranger, on the one hand, and Blankertz, on the other. First, the definition of the relation between provenance and progress in the theories developed by Litt and Spranger is reconstructed and it is shown which education-theoretical assessments it leads to. Then, the - as it were - contrary assessment of this relationship in Blankertz\dbend{} approach is examined. Finally, it is shown in how far both affirmative vanishing points lead to theoretical and legitimacy-related problems and, by way of conclusion, an education-theoretical perspective is presented which keeps equal distance to both vanishing points. (DIPF/Orig.).},
  journal = {Zeitschrift f{\"u}r P{\"a}dagogik},
  keywords = {Bildungstheorie,Blankertz,Eduard,Erkenntnistheorie,Fortschritt,Geisteswissenschaftliche P�dagogik,Herkunft,Herwig,Litt,Spranger,Technik,Theodor,Wissenschaft},
  number = {4}
}

@inproceedings{hardt2017,
