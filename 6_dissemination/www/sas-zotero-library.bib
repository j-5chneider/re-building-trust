@article{krettenauer2004,
  title = {Metaethical Cognition and Epistemic Reasoning Development in Adolescence},
  author = {Krettenauer, Tobias},
  year = {2004},
  volume = {28},
  pages = {461--470},
  issn = {0165-0254},
  doi = {10.1080/01650250444000180},
  abstract = {The present study investigates whether epistemic cognition in moral domain (dubbed metaethical cognition) develops analogously to epistemic reasoning regarding empirical knowledge. The study's conceptual framework distinguishes two main areas of metaethical cognition (beliefs about the nature of moral judgments and conceptions of the process of moral judgment formation), and three metaethical stances (intuitionism, subjectivism, and transsubjectivism). In a sample of 200 adolescents (M 1/4 16.18 years, SD 1/4 2.41), these metaethical stances could be reliably identified by means of a semistructured interview procedure. Adolescents' metaethical stance was related to age, cross-sectionally as well as longitudinally. Furthermore, significant differences in metaethical cognition were found between high school students and an expert group of university students with special training in moral philosophy. Overall, metaethical and epistemic stances were correlated substantially. Findings demonstrate that metaethical reasoning development is a structural analogue of epistemic development regarding factual knowledge. Implications for studies on moral development and for research addressing the domain specificity of epistemic reasoning development are discussed.},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Krettenauer/krettenauer_2004_metaethical_cognition_and_epistemic_reasoning_development_in_adolescence.pdf},
  isbn = {0165-0254},
  journal = {International Journal of Behavioral Development},
  number = {5},
  pmid = {2004402896}
}

@article{krettenauer2005,
  title = {Die {{Erfassung}} Des {{Entwicklungsniveaus}} Epistemologischer {{{\"U}berzeugungen}} Und Das {{Problem}} Der {{{\"U}bertragbarkeit}} von {{Interviewverfahren}} in Standardisierte {{Fragebogenmethoden}}},
  author = {Krettenauer, Tobias},
  year = {2005},
  volume = {37},
  pages = {69--79},
  doi = {10.1026/0049-8637.37.2.69},
  abstract = {The study investigates whether the strategy of translating an interview procedure into a standardized questionnaire proven and tested in the field of moral development by Rest (1979) can be transferred to other research areas of developmental psychology. To this end, a newly developed questionnaire for measuring the developmental level of epistemological beliefs (Fragebogen zur Erfassung des Entwicklungsniveaus epistemologischer {\"U}berzeugungen - FREE) is presented that is based on similar construction principles as the Defining Issues Test (DIT). The reliability and validity of this questionnaire are tested. Cross-sectional and longitudinal analyses demonstrate that the FREE adequately reflects effects of age and education on the development of epistemological beliefs. Furthermore, a substantive correlation between questionnaire and interview data was found. At the same time, however, questionnaire and interview procedures yielded quite different absolute estimates of individuals' competence level. Results are highly consistent with findings obtained in the field of moral development. The FREE, thus, provides a viable alternative to interview procedures for measuring the developmental level of epistemological beliefs as far as the focus is on individual differences rather than individuals' absolute competence level. 
Zusammenfassung. Es wird untersucht, inwieweit die von Rest (1979) im Bereich der Moralentwicklung erprobte Strategie der {\"U}bersetzung von Interviewmethoden in Fragebogenverfahren auf andere Bereiche entwicklungspsychologischer Forschung {\"u}bertragbar ist. Hierf{\"u}r wird ein neu entwickelter Fragebogen zur Erfassung des Entwicklungsniveaus epistemologischer {\"U}berzeugungen (FREE) vorgestellt, der auf vergleichbaren Konstruktionsprinzipien beruht wie der Defining Issues Test (DIT). F{\"u}r diesen Fragebogen werden Reliabilit{\"a}ts- und Validit{\"a}tskennwerte ermittelt. Quer- und l{\"a}ngsschnittliche Analysen weisen eine ausreichende Sensitivit{\"a}t des FREE gegen{\"u}ber Alters- und Bildungseffekten nach. Zudem ergab sich eine hohe korrelative {\"U}bereinstimmung von Fragebogen- und Interviewdaten. Eine deutliche Differenz zwischen beiden Methoden zeigte sich jedoch bei der Einsch{\"a}tzung des absoluten Kompetenzniveaus der Befragten. Die Ergebnisse entsprechen weitgehend den Befunden im Bereich der Moralentwicklung und sprechen f{\"u}r die Generalisierbarkeit der {\"U}bertragungsstrategie. Der FREE empfiehlt sich folglich dann als Alternative zu Interviewverfahren, wenn bei der Untersuchung des Entwicklungsniveaus epistemologischer {\"U}berzeugungen interindividuelle Unterschiede und nicht das absolute Kompetenzniveau im Vordergrund stehen.},
  journal = {Zeitschrift f{\"u}r Entwicklungspsychologie und p{\"a}dagogische Psychologie},
  keywords = {Epistemological beliefs,Measurement}
}

