  author = {Kuhn, Deanna and Park, Seung Ho},
  year = {2005},
  volume = {43},
  pages = {111--124},
  issn = {08830355},
  doi = {10.1016/j.ijer.2006.05.003},
  abstract = {We propose a concept of intellectual values as (a) shared within a group and defined by the extent to which intellectual engagement is regarded as worthwhile to the group's interests, (b) supported by an epistemological belief system, and (c) an important dimension of sociocultural practice and learning. A series of studies is described in which assessments of intellectual values and epistemological beliefs were found to show similar patterns of variation across cultural and subcultural groups of parents and children. Such groups may play a role in highlighting to their youth the intrinsic value of intellectual engagement (versus its instrumental value in achieving individual recognition and status). Intrinsically valued intellectual engagement, we suggest, provides the firmest basis for sustaining intellectual motivation through adolescence and into adulthood. Developing mature epistemological understanding, moreover, is key to this enterprise. \textcopyright{} 2006 Elsevier Ltd. All rights reserved.},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Kuhn et al/kuhn_et_al_2005_epistemological_understanding_and_the_development_of_intellectual_values.pdf},
