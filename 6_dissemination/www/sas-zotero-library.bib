  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Stathopoulou et al/stathopoulou_et_al_2007_exploring_the_relationship_between_physics-related_epistemological_beliefs_and.pdf},
