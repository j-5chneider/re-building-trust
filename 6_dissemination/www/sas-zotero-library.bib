@article{strauss2017,
  title = {{Berufsbezogene {\"U}berzeugungen von angehenden Lehrkr{\"a}ften zur inklusiven Bildung.}},
  author = {Strau{\ss}, Sarah and K{\"o}nig, Johannes},
  year = {2017},
  volume = {45},
  pages = {243--261},
  abstract = {Aufbauend auf systematischen Reviews wird ein neues Rahmenmodell vorgestellt, mit dem verschiedene Ebenen der Begriffsdefinition von Inklusion mit professionellen Anforderungen an Lehrkr{\"a}fte im Bereich inklusiver Bildung in einem Modell verkn{\"u}pft werden. Eine erste Operationalisierung dieses Modells mit sieben neu entwickelten Skalen zu {\"U}berzeugungen von Lehrkr{\"a}ften zu inklusiver Bildung wird vorgestellt und an einer Stichprobe von 409 Lehramtsstudierenden in Form von Strukturanalysen, Messinvarianzanalysen und Subgruppenvergleichen empirisch {\"u}berpr{\"u}ft. F{\"u}r die sieben operationalisierten Konstrukte von {\"U}berzeugungen zeigte sich, dass sie sich reliabel abbilden lie{\ss}en und Ergebnisse der Faktorenanalysen legten nahe, dass {\"U}berzeugungen zu inklusiver Bildung mehrdimensional zu denken sind. Die Pr{\"u}fung, wie sich die Konstrukte im Anforderungsbereich Intervention faktorenanalytisch abbilden lassen, zeigte - wie f{\"u}r das Gesamtmodell - eine bestm{\"o}gliche Anpassung durch die separate Abbildung der einzelnen Konstrukte; ein gemeinsamer Faktor und damit eine eindimensionale {\"U}berzeugung lie{\ss} sich nicht best{\"a}tigen. Insgesamt wiesen die Mittelwerte der hier befragten Lehramtsstudierenden eher positive {\"U}berzeugungen zur Inklusion auf. Die {\"U}berzeugung, dass mit inklusivem Unterricht ein h{\"o}herer Arbeitsaufwand f{\"u}r die Lehrperson verbunden ist, fand ebenfalls eine hohe Zustimmung. Konfigurale und metrische Messinvarianz konnten f{\"u}r die vorliegenden Subgruppen von Lehramtsstudierenden belegt werden. Es zeigte sich, dass Sonderp{\"a}dagogikstudierende f{\"u}r alle Konstrukte bis auf "Orientierung an internal-stabiler Ursachenzuschreibung" signifikant positivere {\"U}berzeugungen haben als die Studierenden der Regelschullehr{\"a}mter. Masterstudierende unterscheiden sich signifikant von den noch nicht so weit fortgeschrittenen Bachelorstudierenden in jenen {\"U}berzeugungen, die einen weiten Inklusionsbegriff adressieren. (ZPID).},
  journal = {Unterrichtswissenschaft},
  keywords = {Behinderter},
  language = {deutsch},
  number = {4}
}

@article{strohmkitchener2002,
  title = {Skills, Tasks, and Definitions: {{Discrepancies}} in the Understanding and Data on the Development of Folk Epistemology},
  author = {Strohm Kitchener, Karen},
  year = {2002},
  volume = {20},
  pages = {309--328},
  issn = {0732118X},
  doi = {10.1016/S0732-118X(02)00012-0},
  abstract = {Claims have been made that similar patterns in the development of folk epistemology occur in children, adolescents, and college students. Some researchers claim that children hold absolutistic assumptions about knowledge that evolve into a kind of relativistic framework where personal interpretation is primary, and last into a rationalistic epistemology. Others suggest the same developmental sequence begins in early adolescence or during the college years. The question addressed in this article is how to make sense of these seemingly discrepant claims and the data supporting them. After initially reviewing the alternative models, Fischer's (Psychol. Rev. 87 (1980) 477-531) skill theory is suggested as a framework for understanding the apparent discrepancies that considers both measurement and time of development issues. Fischer and his colleagues distinguish between "functional level" and "optimal level". Functional level is the level at which most people operate without contextual support or practice. It marks the lower end of the development range. He contrasts this to "optimal level" which marks the upper limit of the person's information processing activity. Performance on epistemological tasks may vary between these two levels. Functional and optimal level data from the reflective judgment model (Developing Reflective Judgment: Understanding and Promoting Intellectual Growth and Critical Thinking in Adolescents and Adults, Jossey-Bass, San Francisco, 1994) are used to illustrate the different levels of folk epistemology expressed under these two conditions and different ages at which they appear. The importance of using consistent, structural descriptions of epistemological levels across different models is emphasized for comparison purposes and, again, skill theory is suggested as one alternative framework for describing differences in structure across levels and reinterpreting the data on epistemological assumptions in children, adolescents, and college students. ?? 2002 Elsevier Science Ltd. All rights reserved.},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Strohm Kitchener/strohm_kitchener_2002_skills,_tasks,_and_definitions_-_discrepancies_in_the_understanding_and_data_on.pdf},
  isbn = {0732-118X},
  journal = {New Ideas in Psychology},
  number = {2-3}
}

