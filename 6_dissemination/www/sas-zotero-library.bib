  arxiv = {NIHMS150003},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Wolf et al/wolf_et_al_2013_sample_size_requirements_for_structural_equation_models_-_an_evaluation_of.pdf},
