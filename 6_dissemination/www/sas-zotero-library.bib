@article{kleinknecht2011,
  title = {Analyse Des Kognitiven {{Aufgabenpotenzials}}. {{Entwicklung}} Und {{Erprobung}} Eines Allgemeindidaktischen {{Auswertungsmanuals}}},
  author = {Kleinknecht, Marc and Maier, Uwe and Metz, Kerstin and Bohl, Thorsten},
  year = {2011},
  volume = {39},
  pages = {328--344},
  abstract = {Im Artikel wird ein allgemeindidaktisches Analyseraster und dessen Erprobung an einer Aufgabenstichprobe dargestellt. Denn w{\"a}hrend in den letzten Jahren in den Fachdidaktiken differenzierte Analysekriterien f{\"u}r Lern- und Testaufgaben entwickelt wurden, die insbesondere auf das kognitive Potenzial von Aufgaben zielen, fehlen f{\"u}r den Bereich der Allgemeinen Didaktik und deren Anwendungsfelder Lehrerbildung sowie Unterrichtsentwicklung fachdidaktisch anschlussf{\"a}hige Kriterien. Ziel der vorliegenden Studie war es deshalb, ein Aufgabenanalysemanual f{\"u}r die fach{\"u}bergreifende Unterrichtsplanung und -reflexion zu entwickeln und dessen Messqualit{\"a}t empirisch zu pr{\"u}fen. Im Zentrum der empirischen Pr{\"u}fung der Messqualit{\"a}t stand die Frage, ob dieses Analyseinstrument bei einer Anwendung auf 127 Aufgaben zu zuverl{\"a}ssigen {\"U}bereinstimmungen zwischen vier Ratern f{\"u}hrt. F{\"u}r eine theoretische Herleitung dieses allgemeindidaktischen Analysemanuals werden aktuelle Aufgabenanalyseraster aus allgemeindidaktischer und fachdidaktischer Perspektive beschrieben, um schlie{\ss}lich das aus sieben Analysedimensionen (Wissensart, Kognitiver Prozess, Wissenseinheiten, Offenheit, Lebensweltbezug, Sprachlogische Komplexit{\"a}t, Repr{\"a}sentationsformen) bestehende Auswertungsmanual zu entfalten. Im Weiteren werden das methodische Vorgehen und die Ergebnisse der empirischen Kriterien{\"u}berpr{\"u}fung dargestellt. Die Messqualit{\"a}t des Instruments erwies sich in weiten Teilen als gut bis sehr gut. Die Weiterentwicklung des Instruments sowie dessen Einsatz bei der Professionalisierung von Lehrkr{\"a}ften werden diskutiert. (ZPID).},
  journal = {Unterrichtswissenschaft},
  keywords = {Bildungsqualit{ä}t;Evaluation;Kognitiver Prozess;S},
  number = {4}
}

