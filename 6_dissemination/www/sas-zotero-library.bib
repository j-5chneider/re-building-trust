  author = {Engberding, Margarita and Hocker, Anna and Nieroba, Sarah and Rist, Fred},
  volume = {21},
  pages = {255--261},
  issn = {1016-6262},
  doi = {10.1159/000333392},
  abstract = {Procrastination (pathologically postponing work on important tasks) is a clinically relevant disorder of self-control. Patients with this disorder do not complete personally relevant tasks in time or not at all, or only under extreme pressure. Common treatment approaches combine various elements from psycho-educative and cognitive-behavioral interventions. In contrast, we present an approach focusing mainly on restricting working time as a treatment method for procrastination. Patients are allowed to work only during certain time windows and only for a set amount of time. Only by increasing their working efficiency, i.e. by utilizing the previously allotted working time as efficiently as possible, they can earn more working time. Restricting the working time leads to a change of perspective from 'I have to study (continuously)' to 'I am allowed to study (right now)'. This short intervention of only 5 sessions can be delivered in a group setting or in an individual setting. Here, we will present its application within a group setting. An evaluation of restricting working time as a group intervention produced convincing effect sizes in the self-assessment instruments as well as with respect to daily study and working behavior assessed via an online working diary. (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  journal = {Verhaltenstherapie},
  keywords = {Human},
  number = {4}
}

