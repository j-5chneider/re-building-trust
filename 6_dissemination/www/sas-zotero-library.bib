  keywords = {:Multilevel structural equation models,adaptive quadrature,Adaptive quadrature,empirical Bayes,Empirical Bayes,factor models,Factor models,generalized linear,Generalized linear mixed models,GLLAMM,hierarchical models,Hierarchical models,item,Item response theory,latent variables,Latent variables,mixed models,Multilevel structural equation models,random effects,Random effects,response theory},
  note = {Series Title: Handbook of Computing and Statistics with Applications}
}

