@article{kippers2018a,
  title = {Data Literacy: {{What}} Do Educators Learn and Struggle with during a Data Use Intervention?},
  author = {Kippers, Wilma B. and Poortman, Cindy L. and Schildkamp, K and Visscher, Adrie J.},
