  author = {Brandt, Mark J. and Ijzerman, Hans and Dijksterhuis, Ap and Farach, Frank J. and Geller, Jason and {Giner-Sorolla}, Roger and Grange, James A. and Perugini, Marco and Spies, Jeffrey R. and {van 't Veer}, Anna},
