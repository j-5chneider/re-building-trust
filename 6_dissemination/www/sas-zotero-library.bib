  eprinttype = {arxiv},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Cleveland et al/cleveland_et_al_1984_graphical_perception_-_theory,_experimentation,_and_application_to_the.pdf},
