@article{thon2017,
  title = {Believing in {{Expertise}}: {{How Authors}}' {{Credentials}} and {{Language Use Influence}} the {{Credibility}} of {{Online Health Information}}},
  author = {Thon, Franziska M. and Jucks, Regina},
