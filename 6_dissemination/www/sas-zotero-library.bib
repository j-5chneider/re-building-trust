@article{lahtinen2013,
  title = {`{{Seeing}} Things in a New Light,: Conditions for Changes in the Epistemological Beliefs of University Students},
  author = {Lahtinen, Aino-Maija and Pehkonen, Leila},
  year = {2013},
  month = may,
  volume = {37},
  pages = {397--415},
  publisher = {{Routledge}},
  issn = {0309-877X},
  doi = {10.1080/0309877X.2011.645458},
  abstract = {The study describes the advancement of the epistemological beliefs of university students. After obtaining interesting research results on changes in students? (N = 170) conceptions of learning from Jan Meyer?s Reflections on Learning Inventory, we carried out a qualitative study using a focus group interview of seven students. The purpose was to gain a deeper understanding of the nature of changes in students? conceptions of learning. However, the initial analysis of the data directed our attention to the epistemological aspects, and we continued the analysis by applying Bendixen and Rule?s Integrative Personal Epistemology Model. The students? educational experiences showed that their epistemological beliefs changed over a period of time from absolutism towards relativism and that the pedagogical environment played an important role in this process. On the basis of the study, we suggest that it is already possible to influence the advancement of university students? epistemological beliefs at an early stage in their studies.},
  journal = {Journal of Further and Higher Education},
  keywords = {FAIQ},
  number = {3}
}

