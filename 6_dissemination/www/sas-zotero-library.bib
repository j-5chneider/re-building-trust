@article{list2016,
  title = {Undergraduate {{Students}}' {{Justifications}} for {{Source Selection}} in a {{Digital Academic Context}}},
  author = {List, Alexandra and Grossnickle, Emily M. and Alexander, Patricia A.},
  year = {2016},
  volume = {54},
  issn = {15414140},
  doi = {10.1177/0735633115606659},
  abstract = {To complete any academic tasks using information from the Internet,\textbackslash{}nundergraduate students first have to select the appropriate sources.\textbackslash{}nHowever, the types of justifications that undergraduates provide for\textbackslash{}nsource selection and how these justifications may be impacted by task\textbackslash{}ncharacteristics have been underexamined. This study explored\textbackslash{}nundergraduates' reported justifications for source selection when\textbackslash{}nresponding to questions in a digital academic context. Participants were\textbackslash{}nfirst asked to answer two questions, one discrete and one open-ended,\textbackslash{}nusing an online library of eight sources varying in type and\textbackslash{}nreliability. Subsequently, a guided retrospective interview was used to\textbackslash{}nelicit undergraduates' justifications for source selection. Source\textbackslash{}nselection decisions were coded as epistemic (e.g., concerned with\textbackslash{}nreliability or credibility) or nonepistemic (e.g., concerned with\textbackslash{}nrelevance or accessibility). Undergraduate students' justifications were\textbackslash{}nsignificantly more likely to be nonepistemic than epistemic. Further,\textbackslash{}nthe reasons for selection offered differed when participants responded\textbackslash{}nto the discrete versus open-ended question, to a limited extent.\textbackslash{}nEpistemic justifications for source selection were related to a number\textbackslash{}nof outcome measures, while nonepistemic justifications were not.\textbackslash{}nFindings are discussed in reference to research and practice pertaining\textbackslash{}nto undergraduates' multiple source use and task design.},
  isbn = {0735-6331, 1541-4140},
  journal = {Journal of Educational Computing Research},
  number = {1}
}

