  keywords = {Missing data,Multiple imputation,Structurai equation modeling},
  number = {2}
}

@article{ludtke2007a,
  title = {Simultane {{Analysen}} Auf {{Sch{\"u}ler}}- {{Und Klassenebene}}: {{Eine Demonstration}} Der {{Konfirmatorischen Mehrebenen}}-{{Faktorenanalyse}} Zur {{Analyse}} von {{Sch{\"u}lerwahrnehmungen}} Am {{Beispiel}} Der {{Hausaufgabenvergabe}}},
  author = {L{\"u}dtke, Oliver and Trautwein, Ulrich and Schnyder, Inge and Niggli, Alois},
  year = {2007},
  volume = {39},
  pages = {1--11},
  doi = {10.1026/0049-8637.39.1.1},
  abstract = {Zusammenfassung. \textbackslash{}nDaten in der p{\"a}dagogisch-psychologischen Forschung weisen h{\"a}ufig eine Mehrebenenstruktur auf: Sch{\"u}ler sind \textbackslash{}ngeschachtelt innerhalb von Schulen und Klassen. Dies muss ber{\"u}cksichtigt werden, wenn Sch{\"u}ler Merkmale ihres schulischen Kon- \textbackslash{}ntextes (z.B. Lehrer, Schulklima) auf mehreren Dimensionen beurteilen sollen und dann die Einsch{\"a}tzungen der Sch{\"u}ler sowohl auf \textbackslash{}nder Individual- als auch aggregiert auf der Klassenebene in weiteren Analysen verwendet werden. Die konfirmatorische Mehrebenen- Faktorenanalyse ist ein Verfahren, das zur Analyse solcher Daten besonders gut geeignet, bislang jedoch noch nicht ausreichend \textbackslash{}nbekannt ist. Die vorliegende Arbeit demonstriert anhand eines Datensatzes aus der Hausaufgabenforschung (1712 Sch{\"u}ler in 98 \textbackslash{}nKlassen) die Grundlagen und Anwendung der Mehrebenen-Faktorenanalyse. Sowohl auf Sch{\"u}ler- als auch auf Klassenebene lie{\ss}en \textbackslash{}nsich die drei Faktoren Qualit{\"a}t der Hausaufgaben, die Kontrolle der Hausaufgaben sowie die effektive Zeitnutzung im Franz{\"o}sisch- \textbackslash{}nUnterricht erwartungsgem{\"a}{\ss} empirisch trennen. Dar{\"u}ber hinaus lie{\ss} sich sowohl auf der Sch{\"u}ler- als auch der Klassenebene ein \textbackslash{}npositiver Effekt der Qualit{\"a}t der Hausaufgaben und der effektiven Zeitnutzung auf die selbst berichtete Anstrengung bei der Bearbei- \textbackslash{}ntung der Hausaufgaben nachweisen. \textbackslash{}n\textbackslash{}nSimultaneous analyses at student and class level: A demonstration of multilevel confirmatory factor analysis of student perceptions of homework assignment \textbackslash{}nAbstract. \textbackslash{}nData in educational research commonly have a multilevel structure -- students are nested within classes and schools. Nested \textbackslash{}nstructures are particularly salient when students are asked to rate aspects of the school context (e.g., teachers, school climate) on \textbackslash{}nseveral dimensions, and these student ratings are then used for further analyses of both the individual level and the aggregated class \textbackslash{}nlevel. Confirmatory multilevel factor analysis is particularly suited to analyzing data of this kind, but this method is not yet sufficient- \textbackslash{}nly well established. In this paper, we demonstrate the principles and application of multilevel factor analysis using a dataset from \textbackslash{}nhomework research (1712 students in 98 classes). As expected, the three factors under investigation (homework quality, teachers' \textbackslash{}nhomework control, and effective use of learning time in French lessons) were empirically distinguishable at both the student and the \textbackslash{}nclass level. Moreover, homework quality and effective use of learning time proved to have positive effects on self reported homework \textbackslash{}neffort at both the student and the class level. \textbackslash{}n},
  file = {/Users/samuelmerk/Google Drive (1)/Uni_gdrive/zotero-library/AUTH/Lüdtke et al/ludtke_et_al_2007_simultane_analysen_auf_schuler-_und_klassenebene_-_eine_demonstration_der.pdf},
  journal = {Zeitschrift fur Entwicklungspsychologie und Padagogische Psychologie},
  keywords = {Factor analysis,Homework,Multilevel analysis,Structural equation modelling,Student perceptions},
  number = {1}
}

